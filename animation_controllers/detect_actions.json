					}],
					"on_entry": [
						"/tag @s remove dead",
						"/tellraw @a[tag=packetlogger] {\"rawtext\":[{\"text\":\"§r§uRosh §j> Received §uDeath§j-§uPacket§j from §8\"},{\"selector\":\"@s\"},{\"text\":\" §j- {stop}\"}]}"
					]
