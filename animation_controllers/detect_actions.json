					]
				},
				"move": {
					"transitions": [{
						"default": "!query.is_moving"
					}],
					"on_entry": [
						"/tag @s add moving",
						"/tellraw @a[tag=packetlogger] {\"rawtext\":[{\"text\":\"§r§uRosh §j> Received §uMove§j-§uPacket§j from §8\"},{\"selector\":\"@s\"},{\"text\":\" §j- {start}\"}]}",
						"/tp @s[tag=freeze] ~~~",
						"/tp @s[y=-105,dy=-205] ~ -104 ~",
