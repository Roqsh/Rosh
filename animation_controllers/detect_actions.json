				"dead": {
					"transitions": [{
						"default": "query.is_alive"
					}],
					"on_entry": [
						"/tag @s add dead",
						"/tag @s remove right",
						"/tellraw @a[tag=packetlogger] {\"rawtext\":[{\"text\":\"§r§uRosh §j> Received §uDeath§j-§uPacket§j from §8\"},{\"selector\":\"@s\"},{\"text\":\" §j- {start}\"}]}"
