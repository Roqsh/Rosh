					"transitions": [{
						"default": "!query.is_sneaking"
					}],
					"on_entry": [
						"/tag @s add sneak",
						"/tellraw @a[tag=packetlogger] {\"rawtext\":[{\"text\":\"§r§uRosh §j> Received §uSneak§j-§uPacket§j from §8\"},{\"selector\":\"@s\"},{\"text\":\" §j- {start}\"}]}"
					]
				}
			}
		},
